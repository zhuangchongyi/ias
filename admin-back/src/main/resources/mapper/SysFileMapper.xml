<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zcy.ias.mapper.SysFileMapper">
    <sql id="baseSql">
        select id,
               file_name,
               file_size,
               file_type,
               file_id,
               file_data,
               create_id,
               create_by,
               create_time,
               update_id,
               update_by,
               update_time,
               del_flag
        from sys_file
    </sql>
</mapper>

